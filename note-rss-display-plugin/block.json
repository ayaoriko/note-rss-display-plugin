{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "note-rss/block",
	"version": "1.0.0",
	"title": "note.com RSS Display Block",
	"category": "media",
    "keywords": ["note", "rss", "feed"],
    "icon": "edit-large",
	"description": "note.comのRSSフィードを表示するブロックです。",
	"attributes": {
		"feed_url": {
			"type": "string",
			"default": ""
		},
		"items": {
			"type": "number",
			"default": 10
		},
		"slide": {
		"type": "string",
		"default": "on"
		}
	},
	"supports": {
		"html": false,
        "align": true
	},
	"textdomain": "note-rss-block",
	"editorScript": "file:./build/index.js",
	"render": "render_note_rss_block"
}